      <div id="home">
         <h4 style="text-align: center" id="printAgent"></h4>
         <style>
            div.tab {
              float: left;
              border: 1px solid #ccc;
              background-color: #f1f1f1;
              width: 30%;
              height: 500px;
              text-align: center;
              padding:10px 0;
            }
            /* Style the buttons inside the tab */
            div.tab button {
            display: block;
            background-color: inherit;
            color: black;
            padding: 10px 0px 10px 0px;
            width: 100%;
            border: none;
            outline: none;
            text-align: left;
            cursor: pointer;
            transition: 0.3s;
            font-size: 14px;
            text-align: center;
            font-weight: bold;
            }
            /* Change background color of buttons on hover */
            div.tab button:hover {
            background-color: #ddd;
            }
            /* Create an active/current "tab button" class */
            div.tab button.active {
            background-color: #ccc;
            }
            /* Style the tab content */
            .tabcontent {
              float: left;
              padding: 12px 12px;
              border: 1px solid #ccc;
              width: 70%;
              border-left: none;
              height: 500px;
              display:block;
              background-color: lightblue;
              overflow: scroll;
              display: none;
            }
            #searchBox {
              padding-left: 10px;
              float: left;
              border: 1px solid #ccc;
              width: 70%;
              border-left: none;
              display:block;
            }
            .topright{
               float:right;
            }
            p {
               padding-left: 2px;
            }
            .reply{
               float:top;
            }
            .list-group{
              float: left;
              padding: 12px 12px;
              border: 1px solid #ccc;
              width: 100%;
              border-left: none;
              height: 95%;
              display:block;
              overflow: scroll;
            }
            .tablinks{
               padding-left: 10px
            }
            .sortlinks {
              margin: 5px;
            }
         </style>
         </head>

         <div class="tab">
            <h4 class="tablinks_head">Users List</h4>
            <ol class="list-group" id="clients">
              <b>LOADING...</b>
            </ol>
         </div>

         <section id="sortBar">
            <div>
              <button type="button" class="sortlinks" id="sortAvg">sort by average chat times</button>
              <button type="button" class="sortlinks" id="sortTotal">sort by total chat times</button>
              <button type="button" class="sortlinks" id="sortFirst">sort by first chat time</button>
              <button type="button" class="sortlinks" id="sortRecent">sort by recent chat time</button>
            </div>
         </section>

        <input type="text" id="searchBox" placeholder="Search for messages.." width="100%">

         <section id="canvas">
         </section>


      </div>
